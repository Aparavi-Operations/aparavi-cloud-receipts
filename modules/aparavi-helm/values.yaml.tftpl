mysql:
  enabled: false
externalMysql:
  hostname: ${mysqlHostname}
  port: ${mysqlPort}
appagent:
  config:
    mysql:
      username: ${mysqlUsername}
      password: ${format("%q", mysqlPassword)}
    platform:
      host: ${platformHost}
      nodeId: ${platformNodeId}
    nodeName: ${appagentNodeName}
  nodeSelector:
    ${appagentNodeSelector}
